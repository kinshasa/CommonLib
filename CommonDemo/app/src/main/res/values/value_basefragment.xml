    <!-- 2015.10.21 add @liushaobo -->
    <style name="wrap_view">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="full_view">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="fullline_view">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="fulldriver_view">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1dp</item>
        <item name="android:background">@color/ui_driver_grap2</item>
    </style>

    <style name="fulldriver_view_0_5">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dp</item>
        <item name="android:background">@color/ui_viewbg_grap3</item>
    </style>

    <style name="verticaldriver_nocolor">
        <item name="android:layout_width">1dp</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <style name="horizontaldriver_nocolor_3dp">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">3dp</item>
    </style>

    <style name="ll_top">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <style name="ll_body">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">5dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/ui_pagebg_grap1</item>
    </style>


    <style name="ll_gb_loading">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>


